<?xml version="1.0" encoding="utf-8" ?>
<Application
    x:Class="MT.App"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="Primary">#ff7300</Color>
            <Color x:Key="Accent">#008cff</Color>
            <Color x:Key="Safe">#00ff73</Color>
            <Color x:Key="Info">#ff008c</Color>
            <Color x:Key="Danger">#ff000c</Color>
            <Color x:Key="Warning">#fff200</Color>
            <Color x:Key="Background">#eae4db</Color>
        </ResourceDictionary>
    </Application.Resources>
</Application>